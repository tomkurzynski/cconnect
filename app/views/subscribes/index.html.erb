<div class="w3-container w3-padding-64 w3-center" id="team">
  <br><br><br><br>
  <p id="notice"><%= notice %></p>
  
  <h1>Subscribes</h1>
  
  <table class="w3-table-all w3-hoverable w3-mobile w3-half ">
    <thead>
      <tr>
        <th>Email</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @subscribes.each do |subscribe| %>
        <tr>
          <td><%= subscribe.email %></td>
          <td><%= link_to '<i class="fas fa-eye fa-lg" title="View"></i>'.html_safe, subscribe %></td>
          <td><%= link_to '<i class="fas fa-edit fa-lg" title="Edit"></i>'.html_safe, edit_subscribe_path(subscribe) %></td>
          <td><%= link_to '<i class="fas fa-trash-alt fa-lg" title="Delete"  ></i>'.html_safe, subscribe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>
  

</div>
  <p><%= link_to 'New Subscribe', new_subscribe_path %></p>