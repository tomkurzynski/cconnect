<div class="w3-container w3-padding-64 w3-center" id="team">
  <p id="notice"><%= notice %></p>
  
  <h1>Shippings</h1>
  
  <table class="w3-table-all w3-hoverable w3-mobile ">
    <thead>
      <tr>
        <th>Client ID</th>
        <th>Line 1</th>
        <th>Line 2</th>
        <th>Line 3</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @shippings.each do |shipping| %>
        <tr>
          <td><%= shipping.clientId %></td>
          <td><%= shipping.Line_1 %></td>
          <td><%= shipping.Line_2 %></td>
          <td><%= shipping.Line_3 %></td>
          <td><%= link_to '<i class="fas fa-eye fa-lg" title="View"></i>'.html_safe, shipping %></td>
          <td><%= link_to '<i class="fas fa-edit fa-lg" title="Edit"></i>'.html_safe, edit_shipping_path(shipping) %></td>
          <td><%= link_to '<i class="fas fa-trash-alt fa-lg" title="Delete"  ></i>'.html_safe, shipping, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>


  <%= link_to 'Add new address', new_shipping_path %>

</div>