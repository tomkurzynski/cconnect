 <% if user_signed_in? %>
    
   

        <div class="w3-container w3-padding-64 w3-center" id="team">
            <h1>Shipping address</h1>
            
            
            
                
                
                
            <table class="w3-table-all w3-hoverable w3-hide-small">
                <thead>
                  <tr>
                    <th>Line 1</th>
                    <th>Line 2</th>
                    <th>Line 3</th>
                    <th>Postcode</th>
                   
                    
                  </tr>
                </thead>
              
                <tbody>
                  <% @users.each do |user| %>
                    <tr>
                      <td><%= user.line1 %></td>
                      <td><%= user.line2 %></td>
                      <td><%= user.line3 %></td>
                      <td><%= user.postcode %></td>
                      
                      
                    </tr>
                  <% end %>
                </tbody>
              </table>
            
            
        </div>
        
       <!--<%= render 'form', edit: @users %>-->
  


<% end %>